  <SQLQuery>
    <![CDATA[SELECT DISTINCT 
       null saleorder_id,
       null ORDER_NUMBER,
       MMODA.FINAL_MFG_ORG_ID ORG_ID,
       MMOSL.BPO cust_po_number,
       NULL HEADER_ID,
       NULL end_user_desc_c,
       MMOH.BRAND brand_c,
       MMOH.STYLE style_no_c,
       NULL style_name_c,
       MMOH.SEASON season_c,
       (select ac.CUSTOMER_NAME
          from ar_customers ac
         where ac.CUSTOMER_ID = MMOH.BUYER_D ) buyer_name,
       NULL sale_order_number,
       MMOH.BUYER_D BUYER_ID 
  FROM MNJ_MFG_OSP_DEP_ASSIGN  MMODA ,
       MNJ_MFG_OSP_STN_LINE    MMOSL   ,
       MNJ_MFG_OSP_HEADER      MMOH
 WHERE MMODA.STN_LINE_ID = MMOSL.STN_LINE_ID
   AND MMOSL.HEADER_ID   = MMOH.HEADER_ID
   AND FINAL_DEPARTMENT = 'Washing'
   and MMODA.FINAL_MFG_ORG_ID = :p_org_id]]>
  </SQLQuery>
