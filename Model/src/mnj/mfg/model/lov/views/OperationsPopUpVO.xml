  <SQLQuery>
    <![CDATA[SELECT QR.CHARACTER6 OPERATION_CODE, 
       QR.CHARACTER2 MAJOR_OPERATION, 
       QR.CHARACTER3 MINOR_OPERATION,
       QR.CHARACTER5 SAM,
       QR.CHARACTER4 MachineType,
       QR.CHARACTER1 DeptName
  FROM QA_RESULTS QR
 WHERE QR.PLAN_ID = (SELECT PLAN_ID
                       FROM QA_PLANS QP
                      WHERE QP.NAME = 'OPERATION MASTER')
   AND QR.CHARACTER1 = 'Washing'                   
ORDER BY QR.CHARACTER6,QR.CHARACTER2,QR.CHARACTER3]]>
  </SQLQuery>
